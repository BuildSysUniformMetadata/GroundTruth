@prefix custom: <http://ucsd.edu/schema/building/power#> .
@prefix ebu3b: <http://ucsd.edu/building/ebu3b#> .
@prefix gbxml: <http://www.gbxml.org/schema_doc/6.01/GreenBuildingXML_Ver6.01#> .
@prefix quantity: <http://qudt.org/vocab/quantity#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix saref: <http://ontology.tno.nl/saref#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix unit: <http://qudt.org/1.1/vocab/unit#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ebu3b:e2523 a saref:EnergyMeter ;
    rdfs:label "EBU3B Mains B Power Meter" ;
    ssn:hasMeasurementProperty ebu3b:e2523_energy,
        ebu3b:e2523_power .

ebu3b:e2524 a saref:EnergyMeter ;
    rdfs:label "EBU3B Mains A Power Meter" ;
    ssn:hasMeasurementProperty ebu3b:e2524_energy,
        ebu3b:e2524_power .

ebu3b:e2523_energy ssn:MeasurementProperty quantity:EnergyAndWork ;
    custom:hasUnit unit:Kilowatthour .

ebu3b:e2523_power ssn:MeasurementProperty quantity:ElectricPower ;
    custom:hasUnit unit:Kilowatt .

ebu3b:e2524_energy ssn:MeasurementProperty quantity:EnergyAndWork ;
    custom:hasUnit unit:Kilowatthour .

ebu3b:e2524_power ssn:MeasurementProperty quantity:ElectricPower ;
    custom:hasUnit unit:Kilowatt .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#area

:area rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      
      rdfs:domain :Location ;
      
      rdfs:range xsd:double .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#hasIFCID

:hasIFCID rdf:type owl:DatatypeProperty .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#height

:height rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        
        rdfs:domain :Location .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#length

:length rdf:type owl:DatatypeProperty ;
        
        rdfs:domain :Location .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#width

:width rdf:type owl:DatatypeProperty ;
       
       rdfs:domain :Location .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#x

:x rdf:type owl:DatatypeProperty ,
            owl:FunctionalProperty ;
   
   rdfs:domain :Location ;
   
   rdfs:range xsd:double .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#y

:y rdf:type owl:DatatypeProperty ,
            owl:FunctionalProperty ;
   
   rdfs:domain :Location ;
   
   rdfs:range xsd:double .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#z

:z rdf:type owl:DatatypeProperty ,
            owl:FunctionalProperty ;
   
   rdfs:domain :Location ;
   
   rdfs:range xsd:double .





#################################################################
#
#    Classes
#
#################################################################


###  http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest

<http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest> rdf:type owl:Class .



###  http://www.ibm.com/SCTC/ontology/PhysicalProcess#FeatureLink

phy:FeatureLink rdf:type owl:Class .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Bank

:Bank rdf:type owl:Class ;
      
      rdfs:subClassOf :Space .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Box

:Box rdf:type owl:Class ;
     
     rdfs:subClassOf :Space .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Building

:Building rdf:type owl:Class ;
          
          rdfs:subClassOf :InsideBuilding ;
          
          rdfs:comment "A building represents a structure that provides shelter for its occupants or contents and stands in one place. The building is also used to provide a basic element within the spatial structure hierarchy for the components of a building project (together with site, storey, and space). " ;
          
          rdfs:seeAlso gbXML:Building ,
                       "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcproductextension/lexical/ifcbuilding.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#BuildingElement

:BuildingElement rdf:type owl:Class ;
                 
                 rdfs:subClassOf phy:FeatureLink ,
                                 :InsideBuilding ;
                 
                 rdfs:comment "The building element comprises all elements that are primarily part of the construction of a building, i.e., its structural and space separating system." ;
                 
                 rdfs:seeAlso "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcproductextension/lexical/ifcbuildingelement.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#BuildingStorey

:BuildingStorey rdf:type owl:Class ;
                
                rdfs:subClassOf :InsideBuilding ;
                
                rdfs:comment "The building storey has an elevation and typically represents a (nearly) horizontal aggregation of spaces that are vertically bound." ;
                
                rdfs:seeAlso gbXML:BuildingStorey ,
                             "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcproductextension/lexical/ifcbuildingstorey.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Ceiling

:Ceiling rdf:type owl:Class ;
         
         rdfs:subClassOf :BuildingElement .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#City

:City rdf:type owl:Class ;
      
      rdfs:subClassOf :Location .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Door

:Door rdf:type owl:Class ;
      
      rdfs:subClassOf :BuildingElement .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Facade

:Facade rdf:type owl:Class ;
        
        rdfs:subClassOf :Outside .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Ground

:Ground rdf:type owl:Class ;
        
        rdfs:subClassOf :Outside .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Groundfloor

:Groundfloor rdf:type owl:Class ;
             
             rdfs:subClassOf :Ceiling .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#InsideBuilding

:InsideBuilding rdf:type owl:Class ;
                
                rdfs:subClassOf :Location .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Location

:Location rdf:type owl:Class ;
          
          rdfs:subClassOf <http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest> ,
                          <http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalPlace> ;
          
          rdfs:comment "Any object that relates to a geometric or spatial context. Subtypes of IfcProduct usually hold a shape representation and a object placement within the project structure." ,
                       "Markers providing information about the location"@en ;
          
          rdfs:seeAlso gbXML:Location ,
                       "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifckernel/lexical/ifcproduct.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Opening

:Opening rdf:type owl:Class ;
         
         rdfs:subClassOf :BuildingElement ;
         
         rdfs:comment "A virtual building element to indicate that two locations are directly connected without a wall, door, etc."@en .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Outside

:Outside rdf:type owl:Class ;
         
         rdfs:subClassOf :Location ;
         
         rdfs:comment "Associated with outside air, often used to model ahu points." ;
         
         rdfs:seeAlso haystack:outside .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Project

:Project rdf:type owl:Class ;
         
         rdfs:subClassOf :Location ;
         
         rdfs:comment "IfcProject indicates the undertaking of some design, engineering, construction, or maintenance activities leading towards a product. The project establishes the context for information to be exchanged or shared, and it may represent a construction project but does not have to. The IfcProject's main purpose in an exchange structure is to provide the root instance and the context for all other information items included." ;
         
         rdfs:seeAlso "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifckernel/lexical/ifcproject.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Roof

:Roof rdf:type owl:Class ;
      
      rdfs:subClassOf :Ceiling .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Rooftop

:Rooftop rdf:type owl:Class ;
         
         rdfs:subClassOf :Outside ;
         
         rdfs:comment "Used with ahu to mark an AHU as a packaged rooftop unit (RTU)." ;
         
         rdfs:seeAlso haystack:rooftop ,
                      "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcsharedbldgelements/lexical/ifcroof.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Room

:Room rdf:type owl:Class ;
      
      rdfs:subClassOf :Space .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Site

:Site rdf:type owl:Class ;
      
      rdfs:subClassOf :Location ;
      
      rdfs:comment "A site is a defined area of land, possibly covered with water, on which the project construction is to be completed." ,
                   "Building or other type of facility with unique street address." ;
      
      rdfs:seeAlso haystack:site ,
                   gbXML:Campus ,
                   "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcproductextension/lexical/ifcsite.htm" ,
                   "http://www.w3.org/ns/org#Site" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Space

:Space rdf:type owl:Class ;
       
       rdfs:subClassOf :Zone ;
       
       rdfs:comment "A space represents a volume enclosed by surfaces." ;
       
       rdfs:seeAlso gbXML:Space ,
                    "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcproductextension/lexical/ifcspace.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Trunk

:Trunk rdf:type owl:Class ;
       
       rdfs:subClassOf :Space .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Wall

:Wall rdf:type owl:Class ;
      
      rdfs:subClassOf :BuildingElement ;
      
      rdfs:comment "The wall represents a vertical construction that bounds or subdivides spaces. Wall are usually vertical, or nearly vertical, planar elements, often designed to bear structural loads. A wall is however not required to be load bearing." ;
      
      rdfs:seeAlso "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcsharedbldgelements/lexical/ifcwall.htm" .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Window

:Window rdf:type owl:Class ;
        
        rdfs:subClassOf :BuildingElement .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Wing

:Wing rdf:type owl:Class ;
      
      rdfs:subClassOf :Space .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#Zone

:Zone rdf:type owl:Class ;
      
      rdfs:subClassOf :InsideBuilding ;
      
      rdfs:comment "Associated with a conditioned zone of a building." ;
      
      rdfs:seeAlso haystack:zone ,
                   gbXML:Zone ,
                   "http://www.buildingsmart-tech.org/ifc/IFC2x4/rc3/html/schema/ifcproductextension/lexical/ifczone.htm" .



###  http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalPlace

<http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalPlace> rdf:type owl:Class .






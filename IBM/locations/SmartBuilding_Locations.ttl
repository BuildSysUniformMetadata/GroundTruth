#    Object Properties
#
#################################################################


###  http://www.ibm.com/SCTC/ontology/PhysicalProcess#linksFeature

phy:linksFeature rdf:type owl:ObjectProperty .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#hasSubLocation

:hasSubLocation rdf:type owl:ObjectProperty ;
                
                rdfs:subPropertyOf <http://www.loa-cnr.it/ontologies/DUL.owl#hasPart> ;
                
                owl:inverseOf :isSubLocationOf ;
                
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                
                rdfs:domain :Location ;
                
                rdfs:range :Location .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#isConnectedTo

:isConnectedTo rdf:type owl:ObjectProperty ;
               
               rdfs:subPropertyOf phy:linksFeature ;
               
               rdf:type owl:SymmetricProperty ;
               
               rdfs:domain :BuildingElement ;
               
               rdfs:range :Location ;
               
               rdfs:comment "Spaces that are directly connected, such as a room and its wall"@en .



###  http://www.ibm.com/SCTC/ontology/SmartBuilding#isSubLocationOf

:isSubLocationOf rdf:type owl:ObjectProperty ;
                 
                 rdfs:subPropertyOf <http://www.loa-cnr.it/ontologies/DUL.owl#isPartOf> ;
                 
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 
                 rdfs:domain :Location ;
                 
                 rdfs:range :Location .



###  http://www.loa-cnr.it/ontologies/DUL.owl#hasPart

<http://www.loa-cnr.it/ontologies/DUL.owl#hasPart> rdf:type owl:ObjectProperty .



###  http://www.loa-cnr.it/ontologies/DUL.owl#isPartOf

<http://www.loa-cnr.it/ontologies/DUL.owl#isPartOf> rdf:type owl:ObjectProperty .





